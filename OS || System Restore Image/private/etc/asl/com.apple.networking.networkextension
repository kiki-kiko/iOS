= enable [Plist /Library/Preferences/com.apple.networkextension.control.plist] [N= LogToFile 1]
? [= Facility com.apple.networking.networkextension] file /Library/Logs/CrashReporter/com.apple.networking.networkextension.log rotate=local-basic crashlog file_max=1M compress format=$((Time)(local.6))\ $Host\ $(Sender)[$(PID)]\ <$((Level)(str))>:\ $(Message)
> /Library/Logs/CrashReporter mode=0755
